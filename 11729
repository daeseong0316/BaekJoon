#정의 _CRT_SECURE_NO_WARNING
#include <stdio.h>
#include <string.h>

주 () {}에서
    int N;  // 숫자의 갯수
    scanf("%d", &N);    // 숫자 갯수 입력

    char N2[101] = {};  // 문자열 형식의 배열 101칸 생성
    scanf("%s", &N2);   // 배열에 숫자 입력
    intcount = strlen(N2); // 배열의 칸 수 

    int sum = 0;    // sum을 0으로 초기화 한 상태
    (inti = 0; i < count; i++) { // 칸 수 만큼 반복의 경우
        sum += (int)N2[i] - 48; // 숫자를 sum에 더하면서 저장
    }

    printf ("%d", sum); // sum 출력

    반환 0;
}
