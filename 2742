import java.util.Arrays;
import java.util.Scanner;

public class Main {

		public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n;
		n = sc.nextInt();
		
		for(int i=n;i>=1;i--) {
			System.out.println(i);
		}
		
		sc.close();
	}

}
